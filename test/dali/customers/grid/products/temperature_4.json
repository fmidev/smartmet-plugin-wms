{
  "title": "Temperature",
  "abstract": "Temperature",
  "producer": "SMARTMET",
  "projection": {
    "crs": "data",
    "size": 1.0
  },
  "views": [
    {
      "layers": [
        {
          "qid": "l1",
          "layer_type": "isoband",
          "parameter": "T-K",
          "isobands": "json:isobands/T-K.json",
          "css": "isobands/temperature.css",
          "smoother": {
            "degree": 1,
            "size": 1
          }
        },
        {
          "qid": "l2",
          "layer_type": "number",
          "css": "numbers/integers.css",
          "parameter": "T-K",
          "offset" : -273.15,
          "attributes": {
            "class": "Number",
            "id": "wind_numbers"
          },
          "label": "json:numbers/integers.json",
          "positions": {
            "x": 15,
            "y": 15,
            "dx": 20,
            "dy": 20,
            "ddx": 10
          }
        },
        {
          "qid": "l10",
          "layer_type": "map",
          "map": {
            "schema": "natural_earth",
            "table": "admin_0_countries"
          },
          "attributes": {
            "id": "europe_country_lines",
            "fill": "none",
            "stroke": "#222",
            "stroke-width": "0.3px"
          }
        }
      ]
    }
  ]
}
