{
    "title": "temperature differently inside precipitation",
    "producer": "pal_skandinavia",
    "language": "fi",
    "projection":
    {
	"crs": "data",
	"xsize": 500,
	"ysize": 500
    },
    "views": [
        {
	    "qid": "v1",
	    "attributes":
	    {
		"id": "view1"
	    },
            "layers": [
                {
		    "qid": "l1",
                    "layer_type": "isoband",
                    "isobands": "json:isobands/temperature.json",
		    "css": "isobands/temperature.css",
                    "parameter": "Temperature",
		    "attributes":
		    {
			"id": "temperature_isobands"
		    }
		},
                {
		    "qid": "l2",
                    "layer_type": "isoband",
                    "isobands": "json:isobands/temperature.json",
		    "css": "isobands/temperature.css",
                    "parameter": "Temperature",
		    "intersect":
		    {
			"parameter": "Precipitation1h",
			"lolimit": 0.1
		    },
		    "attributes":
		    {
			"filter": "url(#grayscale)",
			"id": "temperature_and_rain"
		    }
		},
		{
		    "qid": "l4",
		    "layer_type": "map",
		    "map":
		    {
			"schema": "esri",
			"table": "europe_country_wgs84",
			"minarea": 100
		    },
		    "attributes":
		    {
			"id": "europe_country_lines",
			"fill": "none",
			"stroke": "#222",
			"stroke-width": "0.3px"
		    }
		}
            ]
        }
    ]
}
