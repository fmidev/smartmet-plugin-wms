{
    "title": "HIRLAM wind from direction and speed",
    "producer": "hirwind",
    "projection": {
        "xsize": 1025,
        "ysize": 712,
        "crs": "+proj=laea +lat_0=60 +lon_0=20 +units=m +datum=WGS84",
        "bboxcrs": "EPSG:4326",
        "cy": 53,
        "cx": 5,
        "resolution": 6
    },
    "views": [
        {
            "layers": [
                {
                    "qid": "map",
                    "layer_type": "map",
                    "map": {
                        "schema": "natural_earth",
                        "table": "admin_0_countries",
                        "lines": true
                    },
                    "attributes": {
                        "fill": "none",
                        "stroke": "#222",
                        "stroke-width": "0.3px"
                    }
                },
                {
                    "qid": "graticule",
                    "layer_type": "symbol",
                    "symbol": "dot",
                    "css": "symbols\/dot.css",
                    "positions": {
                        "layout": "graticule",
                        "size": 10,
                        "step": 1
                    }
                },
                {
                    "qid": "wind",
                    "layer_type": "arrow",
                    "arrows": "json:arrows/wind.json",
		    "symbol": "windarrow",
		    "css": "arrows/windarrow.css",
                    "u": "WindUMS",
                    "v": "WindVMS",
                    "attributes": {
                        "id": "wind_arrows"
                    },
                    "positions": {
                        "x": 5,
                        "y": 5,
                        "dx": 30,
                        "dy": 30,
                        "ddx": 15
                    }
                }
            ]
        }
    ]
}
