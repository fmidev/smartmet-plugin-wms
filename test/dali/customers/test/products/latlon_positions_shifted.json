{
    "title": "ECMWF temperatures at some coordinates",
    "producer": "ecmwf_maailma_pinta",
    "projection": {
        "crs": "EPSG:2393",
        "bboxcrs": "EPSG:4326",
        "place": "Berliini",
        "resolution": 8,
        "xsize": 800,
        "ysize": 500
    },
    "views": [
        {
            "qid": "v1",
            "attributes": {
                "id": "view1"
            },
            "layers": [
                {
                    "qid": "l1",
                    "layer_type": "isoband",
                    "isobands": "json:isobands/temperature.json",
                    "css": "isobands/temperature.css",
                    "parameter": "Temperature",
                    "attributes": {
                        "id": "t2m_isobands"
                    }
                },
                {
                    "qid": "l2",
                    "layer_type": "number",
                    "css": "numbers/integers.css",
                    "parameter": "Temperature",
                    "attributes": {
                        "class": "Number",
                        "id": "t2m_numbers"
                    },
                    "label": "json:numbers/integers.json",
                    "positions": {
                        "layout": "latlon",
			"dx": 10,
			"dy": -10,
                        "locations": [
                            {
                                "longitude": 25, // Helsinki
                                "latitude": 60
                            },
                            {
                                "longitude": 18, // Tukholma
                                "latitude": 59
                            },
                            {
                                "longitude": 2, // Barcelona
                                "latitude": 41
                            },
                            {
                                "longitude": 12, // Rooma
                                "latitude": 42
                            },
                            {
                                "longitude": -9, // Lissabon
                                "latitude": 38
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
