{
  "title": "Windbarb demo",
  "producer": "pal_skandinavia",
  "projection": {
    "crs": "data",
    "xsize": 500,
    "ysize": 500
  },
  "views": [
    {
      "qid": "v1",
      "attributes": {
        "id": "view1"
      },
      "layers": [
        {
          "qid": "l1",
          "layer_type": "isoband",
          "isobands": "json:isobands/wind.json",
          "css": "isobands/wind.css",
          "parameter": "WindSpeedMS",
          "attributes": {
            "id": "wind_isobands"
          }
        },
        {
          "qid": "l2",
          "layer_type": "isoline",
          "isolines": "json:isolines/wind.json",
          "css": "isolines/wind.css",
          "parameter": "WindSpeedMS",
          "attributes": {
            "id": "wind_isolines",
            "class": "Wind"
          }
        },
        {
          "qid": "l3",
          "layer_type": "arrow",
          "arrows": "json:arrows/windbarb.json",
          "css": "arrows/windbarb.css",
          "direction": "WindDirection",
	  "multiplier": 1.94384449244,	// to knots, inverse of 0.514444....
	  "southflop": true,
	  "scale": 1.0,
          "speed": "WindSpeedMS",
          "attributes": {
            "id": "wind_arrows"
          },
          "positions": {
            "x": 5,
            "y": 5,
            "dx": 30,
            "dy": 30,
            "ddx": 15
          }
        }
      ]
    }
  ]
}
