{
  // GetCapability test variables
  "title": "Wind demo",
  "abstract": "Test abstract",
  "opaque": false,
  "queryable": true,
  "cascaded": true,
  "no_subsets": true,
  "fixed_width": 1234,
  "fixed_height": 4321,
  "keyword": ["foo", "bar"],

  // Normal settings
  "producer": "pal_skandinavia",
  "legend_url_layer": "test:wind_legend",
  "projection": {
    "crs": "data",
    "xsize": 500,
    "ysize": 500
  },
 "styles": [
    {
	"name": "style1",
	"title": "Title for style1",
	"abstract": "Abstract for style1",
	"legend_url_layer": "test:wind_legend",
	"legend_url_layer_style": "alternative_legend_style1"
    },
    {
	"name": "style2",
	"title": "Title for style2",
	"abstract": "Abstract for style2",
	"legend_url_layer": "test:wind_legend",
	"legend_url_layer_style": "alternative_legend_style2"
    }
 ],
  "views": [
    {
      "qid": "v1",
      "attributes": {
        "id": "view1"
      },
      "layers": [
        {
          "qid": "l1",
          "layer_type": "isoband",
          "isobands": "json:isobands/wind.json",
          "css": "isobands/wind.css",
          "parameter": "WindSpeedMS",
          "attributes": {
            "id": "wind_isobands"
          }
        },
        {
          "qid": "l2",
          "layer_type": "isoline",
          "isolines": "json:isolines/wind.json",
          "css": "isolines/wind.css",
          "parameter": "WindSpeedMS",
          "attributes": {
            "id": "wind_isolines",
            "class": "Wind"
          }
        },
        {
          "qid": "l6",
          "tag": "g",
          "attributes": {
            "filter": "url(#shadow)"
          },
          "layers": [
            {
              "qid": "l6",
              "tag": "rect",
              "attributes": {
                "x": 330,
                "y": 3,
                "width": 155,
                "height": 25,
                "rx": 8,
                "ry": 8,
                "fill": "white",
                "stroke": "black",
                "stroke-width": "0.6"
              }
            },
            {
              "qid": "l7",
              "layer_type": "time",
              "timestamp": "validtime",
              "timezone": "Europe/Helsinki",
	      "format": "%Y-%m-%d %H:%M",
	      "x": -20,
	      "y": 20,
              "attributes": {
                "text-anchor": "end",
                "font-family": "Roboto",
                "font-weight": "bold",
                "font-size": 14
              }
            }
          ]
        }
      ]
    }
  ]
}
