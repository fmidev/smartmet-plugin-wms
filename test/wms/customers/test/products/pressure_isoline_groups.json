{
    "title": "Demo product",
    "producer": "pal_skandinavia",
    "projection":
    {
	"crs": "data",
	"xsize": 500,
	"ysize": 500
    },
    "views": [
        {
	    "qid": "v1",
	    "attributes":
	    {
		"id": "view1"
	    },
            "layers": [
		{
		    "qid": "background",
		    "layer_type": "background",
		    "attributes":
		    {
			"fill": "rgb(0,75,140)"
		    }
		},
		{
		    "qid": "europe",
		    "layer_type": "map",
		    "map":
		    {
			"schema": "natural_earth",
			"table": "admin_0_countries",
			"where": "iso_a2 NOT IN ('FI','AX')"
		    },
		    "attributes":
		    {
			"id": "europe_country",
			"fill": "rgb(120,120,0)"
		    }
		},
		{
		    "qid": "finland",
		    "layer_type": "map",
		    "map":
		    {
			"schema": "natural_earth",
			"table": "admin_0_countries",
			"where": "iso_a2 IN ('FI','AX')"
		    },
		    "attributes":
		    {
			"id": "finland_country",
			"fill": "rgb(120,130,33)"
		    }
		},
                {
		    "qid": "vv1",
		    "tag": "g",
		    "attributes":
		    {
			"filter": "url(#shadow)",
			"id": "precipitation_group"
		    },
		    "layers": [
			{
			    "qid": "l1",
			    "layer_type": "isoband",
			    "isobands": "json:isobands/precipitation.json",
			    "css": "isobands/precipitation.css",
			    "parameter": "Precipitation1h",
			    "attributes":
			    {
				"id": "precipitation_isobands"
			    }
			},
			{
			    "qid": "l2",
			    "layer_type": "isoline",
			    "isolines": "json:isolines/precipitation.json",
			    "css": "isolines/precipitation.css",
			    "parameter": "Precipitation1h",
			    "attributes":
			    {
				"id": "precipitation_isolines",
				"class": "Precipitation"
			    }
			},
			{
			    "qid": "l3",
			    "layer_type": "symbol",
			    "symbols": "json:symbols/precipitationform.json",
			    "parameter": "PrecipitationForm",
			    "scale": 1.0,
			    "positions":
			    {
				"x": 5,
				"y": 5,
				"dx": 20,
				"dy": 20,
				"ddx": 5
			    },
			    "attributes":
			    {
				"id": "precipitation_form",
				"class": "PrecipitationForm"
			    }
			}
		    ]
                },
		{
		    "qid": "europe_lines",
		    "layer_type": "map",
		    "map":
		    {
			"schema": "natural_earth",
			"table": "admin_0_countries"
		    },
		    "attributes":
		    {
			"id": "europe_country_lines",
			"fill": "none",
			"stroke": "#222",
			"stroke-width": "0.3px"
		    }
		},
                {
		    "qid": "l4",
                    "layer_type": "isoline",
                     "isolines": 
		    {  
			"qidprefix": "isoline_main_",
			"startvalue": 900, 
			"endvalue": 1100,
			"interval": 5
		    },
                    "css": "isolines/pressure.css",
                    "parameter": "Pressure",
		    "attributes":
		    {
			"fill": "none",
			"stroke": "#333",
			"stroke-width": "1.5px"
		    }
                },
                {
		    "qid": "l5",
                    "layer_type": "isoline",
                    "isolines": 
		    {  
			"qidprefix": "isoline_sub_",
			"startvalue": 900, 
			"endvalue": 1100,
			"interval": 2,
			"except": 5
		    },
                    "css": "isolines/pressure.css",
                    "parameter": "Pressure",
		    "attributes":
		    {
			"fill": "none",
			"stroke": "red",
			"stroke-dasharray": "10,5",
			"stroke-width": "1.0px"
		    }
		}
            ]
        }
    ]
}
