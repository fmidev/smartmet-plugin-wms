{
    "title": "Demo product",
    "producer": "pal_skandinavia",
    "language": "fi",
    "projection":
    {
	"crs": "data",
	"xsize": 500,
	"ysize": 500
    },
    "views": [
        {
	    "qid": "v1",
	    "attributes":
	    {
		"id": "view1"
	    },
            "layers": [
                {
		    "qid": "l1",
                    "layer_type": "isoband",
                    "isobands": "json:isobands/temperature.json",
		    "css": "isobands/temperature.css",
                    "parameter": "Temperature",
		    "attributes":
		    {
			"id": "temperature_isobands"
		    }
                },
		{
		    "tag": "g",
		    "attributes":
		    {
			"mask": "url(#temperaturelegendmask)"
		    },
		    "layers": [
			{
			    "qid": "l4",
			    "layer_type": "map",
			    "map":
			    {
				"schema": "natural_earth",
				"table": "admin_0_countries",
				"minarea": 100
			    },
			    "attributes":
			    {
				"id": "europe_country_lines",
				"fill": "none",
				"stroke": "#222",
				"stroke-width": "0.3px"
			    }
			},
			{
			    "layer_type": "time",	
			    "timestamp": "validtime",
                            "timezone": "Europe/Helsinki",
                            "format": "%Y-%m-%d %H:%M",
			    "x": -20,
			    "y": 20,
			    "attributes":
			    {
			        "text-anchor": "end",
				"font-family": "Verdana",
				"font-weight": "bold",
				"font-size": 14
			    }
			}
		    ]
		}
            ]
        }
    ]
}
