{
    "title": "RoadCloud obervations for 5 minute period",
    "producer": "roadcloud",
    "timestep": 5,
    "interval_start": 5,
    "interval_end": 0,
    "margin": 5,
    "projection":
    {
    },
    "views": [{
	"layers": [
               {
                    "qid": "sea",
                    "layer_type": "background",
                    "attributes":
                    {
                        "fill": "white"
                    }
                },
                {
                    "qid": "l1",
                    "layer_type": "map",
                    "map":
                    {
                        "schema": "natural_earth",
                        "table": "admin_0_countries",
                        "minarea": 100
                    },
                    "attributes":
                    {
                        "id": "europe_country_lines",
                        "fill": "none",
                        "stroke": "#222",
                        "stroke-width": "0.3px"
                    }
                },
	    {
	    "qid": "l2",
	    "layer_type": "symbol",
	    "css": "symbols/iot_station.css",
	    "parameter": "speed",
	    "symbol": "iot_station",
	    "symbols":
	    [
		{
		    "attributes":
		    {
			"fill": "#00ff00",
			"scale": 0.5
		    }
		}

	    ],
	    "attributes":
	    {
		"class": "IoTStationMarker"
	    }
	   },
	    {
	    "qid": "l3",
	    "layer_type": "symbol",
	    "css": "symbols/iot_station.css",
	    "parameter": "friction",
	    "symbol": "iot_station",
	    "symbols":
	    [
		{
		    "attributes":
		    {
			"fill": "#00ff00",
			"scale": 0.5
		    }
		}

	    ],
	    "attributes":
	    {
		"class": "IoTStationMarker"
	    }
	   },
	    {
	    "qid": "l4",
	    "layer_type": "symbol",
	    "css": "symbols/iot_station.css",
	    "parameter": "road_state",
	    "symbol": "iot_station",
	    "symbols":
	    [
		{
		    "attributes":
		    {
			"fill": "#00ff00",
			"scale": 0.5
		    }
		}

	    ],
	    "attributes":
	    {
		"class": "IoTStationMarker"
	    }
	   }
	]
    }]
}
