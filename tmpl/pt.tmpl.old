<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"  width="1030px" height="520px" viewBox="0 0 1030 520">
<title>Demo template</title>
<defs>
 <style type="text/css"><![CDATA[

<TMPL_include "styles/default/Pressure.tmpl">
<TMPL_include "styles/default/Temperature.tmpl">

.weather
 {
   filter: url(#shadow);
 }

 ]]></style>

 <TMPL_include "filters/shadow.tmpl">
 <TMPL_include "filters/emboss.tmpl">

 <TMPL_foreach contours.view1 as contour>
 <path id="<TMPL_var id>" d="<TMPL_var path>"/></TMPL_foreach>
 <TMPL_foreach contours.view2 as contour>
 <path id="<TMPL_var id>" d="<TMPL_var path>"/></TMPL_foreach>

</defs>

<g id="view1" transform="translate(10,10)">
<rect x="0" y="0" width="500" height="500" stroke="black"/>

<TMPL_foreach contours.view1 as contour>
<use class="<TMPL_var class>" xlink:href="#<TMPL_var id>"/></TMPL_foreach>
</g>

<g id="view2" transform="translate(520,10)">
<rect x="0" y="0" width="500" height="500" stroke="black" fill="yellow"/>

<TMPL_foreach contours.view2 as contour>
<use class="<TMPL_var class>" xlink:href="#<TMPL_var id>"/></TMPL_foreach>

<TMPL_foreach contours.view2 as contour>
<text font-size="15">
<textPath xlink:href="#<TMPL_var id>">
<tspan dx="10%"><TMPL_var value></tspan>
<tspan dx="10%"><TMPL_var value></tspan>
<tspan dx="10%"><TMPL_var value></tspan>
<tspan dx="10%"><TMPL_var value></tspan>
<tspan dx="10%"><TMPL_var value></tspan>
</textPath>
</text></TMPL_foreach>
</g>

</svg>
